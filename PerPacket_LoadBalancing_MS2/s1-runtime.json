{
  "target": "bmv2",
  "p4info": "build/perpacket.p4.p4info.txt",
  "bmv2_json": "build/perpacket.json",
  "table_entries": [
    {
      "table": "MyIngress.ecmp_group",
      "default_action": true,
      "action_name": "MyIngress.drop",
      "action_params": { }
    },
    {
      "table": "MyIngress.ecmp_group",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.0.1", 32]
      },
      "action_name": "MyIngress.set_ecmp_select",
      "action_params": {
        "ecmp_base": 0,
        "ecmp_count": 2
      }
    },
    {
      "table": "MyIngress.ecmp_nhop",
      "match": {
        "meta.ecmp_select": 0
      },
      "action_name": "MyIngress.set_nhop",
      "action_params": {
        "nhop_ipv4": "10.0.2.2",
        "port" : 2
      }
    },
    {
      "table": "MyIngress.ecmp_nhop",
      "match": {
        "meta.ecmp_select": 1
      },
      "action_name": "MyIngress.set_nhop",
      "action_params": {
        "nhop_ipv4": "10.0.2.2",
        "port" : 3
      }
    }
  ]
}
